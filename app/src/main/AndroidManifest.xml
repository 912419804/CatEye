        <!--android:name="UMENG_CHANNEL"-->
        <!--android:value="${UMENG_CHANNEL}"/>-->
        <!--bugly配置-->
        <!--<meta-data-->
        <!--android:name="BUGLY_APP_CHANNEL"-->
        <!--android:value="<APP_Channel>" />-->
    </application>

</manifest>